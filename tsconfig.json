{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "es2015"],
    "sourceMap": true,
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@antv/l7-extension-g-layer": ["packages/g-layer/src/"],
      "@antv/l7-leaflet": ["packages/leaflet/src/"],
      "district-data": ["packages/district-data/src/"]
    }
  },
  "include": [".dumirc.ts", "packages/**/*", "docs/**/*"]
}
